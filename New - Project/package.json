{
  "name": "mastra-insight-assistant",
  "version": "1.0.0",
  "description": "Production-grade AI-powered Insight Assistant with multi-agent orchestration",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "npx tsx watch src/index.ts",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:all": "node scripts/start-dev.js",
    "dev:all:concurrent": "concurrently \"npm run dev\" \"npm run dev:frontend\" --names \"backend,frontend\" --prefix name --prefix-colors \"cyan,magenta\"",
    "build": "tsc",
    "build:frontend": "cd frontend && npm run build",
    "build:all": "npm run build && npm run build:frontend",
    "start": "node dist/index.js",
    "start:frontend": "cd frontend && npm start",
    "start:all": "concurrently \"npm start\" \"npm run start:frontend\" --names \"backend,frontend\" --prefix name --prefix-colors \"cyan,magenta\"",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:frontend": "cd frontend && npm run lint",
    "lint:all": "npm run lint && npm run lint:frontend",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "install:all": "npm install && cd frontend && npm install"
  },
  "keywords": [
    "mastra",
    "ai",
    "agents",
    "insights",
    "assistant"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "zod": "^3.22.4",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "winston": "^3.11.0",
    "node-cache": "^5.1.2",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/cors": "^2.8.17",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0",
    "@vitest/coverage-v8": "^1.1.0",
    "concurrently": "^8.2.2"
  }
}

